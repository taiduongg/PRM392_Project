<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Selected item -->
    <item android:color="#FF5722" android:state_checked="true" />
    <!-- Pressed but not selected -->
    <item android:color="#FFAB91" android:state_pressed="true" />
    <!-- Default state -->
    <item android:color="#888888" />
</selector>
